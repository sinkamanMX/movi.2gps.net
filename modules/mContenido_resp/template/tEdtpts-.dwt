<script>
	$(function() {
		var st=($("#st").is(':checked'))?1:0;
		var ex=($("#ex").is(':checked'))?1:0;
		var dt= $("#dsp").val()+'¬'+$("#idv").val()+'¬'+$("#und").val()+'¬'+$("#dti").val()+'¬'+$("#dtf").val()+'¬'+$("#tl").val()+'¬'+$("#hri").val()+'¬'+$("#mni").val()+'¬'+$("#hrf").val()+'¬'+$("#mnf").val()+'¬'+st+'¬'+ex;
		$("#data").val(dt);
		$( "#start_date,#end_date,#delivery-date" ).datepicker({
			showOn: "button",
			buttonImage: "public/images/cal.gif",
			buttonImageOnly: true,
			minDate: '0',
			dateFormat: "yy-mm-dd"
		});
		
		//$( "#panelPE" ).tabs();

		$('#panelPE').panel({
			controls:$('#cntrl').html(),
    		collapsible:false
				});
		 $( ".b_ic" ).button();
		 $(".b_op" ).button();
		/*$('#panelE').panel({

			'controls':$('#cntrl').html(),
			'collapsible':false

		});*/
						
	});

	</script>

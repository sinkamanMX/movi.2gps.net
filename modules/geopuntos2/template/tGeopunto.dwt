    <!-- BEGIN U -->
    <div class="ui-corner-all" id="{U.ID}">{U.DES}</div>
    <!-- END U -->
    </div>
    </div>
    </div>   
    <!--cuestionarios disponibles-->
    <!--cuestionarios seleccionados-->
    <div style="width:49%; height:100%; float:right; border:#a6c9e2 solid thin;" class="ui-corner-all">
    <div class="ui-dialog-titlebar ui-widget-header ui-corner-all ui-helper-clearfix ">
    <span class="ui-dialog-title">Cuestionarios Seleccionados</span>

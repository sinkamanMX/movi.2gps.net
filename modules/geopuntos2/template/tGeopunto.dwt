    </div>
    <!--Buscador-->
    <div>
    <div style="width:96%;">
    <label>Buscar:</label><input type="text" id="geo_bscdr_qs" class="caja_txt" onkeyup="geo_buscador_qst(2,this.value)" style="width:90%;"/>
    </div>
    <div style="width:3%; position:relative; left:96%; top:-20px;">
    <span class="ui-icon ui-icon-search" style="position:relative; left:-5px; top:-5px;"></span>
    </div>
    </div>
    <!--Buscador-->
    <!--<div style="overflow-y:scroll; height:90%; width:100%; position:relative;">-->
    <div style="overflow-y:scroll; overflow-x:hidden; width:100%; min-height:350px; max-height:355px; height:353;">
    <div id="geo_qst_sel" class="dropme" style="min-height:350px; min-width:170px; width:100%; border-bottom:#fad42e solid thin; border-top:#fad42e solid thin;">
    <!-- BEGIN U2 -->
    <div class="ui-corner-all" id="{U2.ID}">{U2.DES}</div>
    <!-- END U2 -->
    </div>
    </div><!--scroll-->
    </div>
    <!--</div>-->
    <!--cuestionarios seleccionados-->
    
    </div>
  
  </div><!--tabs3-->
  <div id="geo_tabs4">
  <br><br><br>
  <h2 align="center" style="color:#CCC;">Seleccione al menos un cuestionario</h2>
  </div><!--tabs4-->

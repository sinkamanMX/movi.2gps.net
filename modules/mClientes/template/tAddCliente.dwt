<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<script>
$(document).ready(function () {
	$( "#tabs_add_client" ).tabs();
	});
</script>
<div id="tabs_add_client">
  <ul>
    <li><a href="#gral_client_data_nvo">Datos Generales</a></li>
    <li><a href="#cont_client_data_nvo">Contacto</a></li>
    <li><a href="#div_cli_val">Validación</a></li>
  </ul>
  <div id="gral_client_data_nvo">
  <!--<fieldset><legend>Información</legend>-->

    <table class="tableP">
      <tr>
        <td>Nombre:</td>
        <td><input type="text" id="cli_nvo_name" class="caja_txt" maxlength="60"/></td>
      </tr>
      <tr>
        <td>RFC:</td>
        <td><input type="text" id="cli_nvo_rfc" class="caja_txt" maxlength="60"/></td>
      </tr>
      <tr>
        <td>Raz&oacute;n Social:</td>
        <td><input type="text" id="cli_nvo_rzon" class="caja_txt" maxlength="60"/></td>
      </tr> 
      <tr>
        <td>Persona:</td>
        <td>
        <select id="cli_nvo_per" class="caja">
        <option value="FISICA">FISICA</option>
        <option value="MORAL">MORAL</option>
        </select>
        </td>                           
    </table>
    

 <!-- </fieldset>    -->
  </div><!--gral_client_data_nvo-->
  <div id="cont_client_data_nvo">
  <!--<fieldset><legend>Información</legend>-->

    <table class="tableP">
      <tr>
        <td>Tel&eacute;fono:</td>
        <td><input type="text" id="cli_nvo_tel" class="caja_txt" maxlength="60"/></td>
      </tr>
      <tr>
        <td>Direcci&oacute;n:</td>
        <td><input type="text" id="cli_nvo_dir" class="caja_txt" maxlength="60"/></td>
      </tr>
      <tr>
        <td>Contacto:</td>
        <td><input type="text" id="cli_nvo_con" class="caja_txt" maxlength="60"/></td>
      </tr> 
      <tr>
        <td>e-mail:</td>
        <td><input type="text" id="cli_nvo_ema" class="caja_txt" maxlength="60"/></td>
      </tr>
      <tr>
        <td>Movil:</td>
        <td><input type="text" id="cli_nvo_mov" class="caja_txt" maxlength="60"/></td>
      </tr>      
      
      <tr>
        <td>Activo:</td>
        <td>
        <select id="cli_nvo_act" class="caja">
        <option value="S">Si</option>
        <option value="N">No</option>
        </select>
        </td>                           
    </table>
    

 <!-- </fieldset>     -->
  </div><!--cont_client_data_nvo-->
  <div id="div_cli_val">
    <table>
      <tr>
        <td>Pregunta:</td>
        <td><input type="text" id="cli_edt_preg" class="caja_txt" maxlength="60" value="{PREG}"/></td>
      </tr>      
      <tr>
        <td>Respuesta:</td>
        <td><input type="text" id="cli_edt_resp" class="caja_txt" maxlength="60" value="{RESP}"/></td>
      </tr>
    </table>
  </div>
</div>